//------------------------------------------------------------------------
// 简称: DPO
// 名称: 区间振荡指标
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

Params
	Numeric Length(7);
Vars
	Numeric Offset( 0 ) ;
	NumericSeries AvgClose(0);
	Numeric DPOValue;
Begin
	Offset = Length * 0.5 + 1 ;
	AvgClose = AverageFC( Close, Length );
	DPOValue = Close - AvgClose[Offset] ;	
	PlotNumeric("DPO",DPOValue);
	PlotNumeric("Ref",0);
End


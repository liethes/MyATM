//------------------------------------------------------------------------
// 简称: WR
// 名称: 威廉指标
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

Params
	Numeric Length(5);
	Numeric OverSold(20) ;
	Numeric OverBought(80) ;	
Vars
	Numeric WRValue;
Begin
	WRValue = PercentR(Length);
	PlotNumeric("WR",WRValue);
	PlotNumeric("超买",OverBought);
	PlotNumeric("超卖",OverSold);
End


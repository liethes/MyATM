//------------------------------------------------------------------------
// 简称: ROC
// 名称: 变动速率
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

Params
	Numeric Length(12);
Vars
	Numeric ROCValue;
Begin
	ROCValue = (Close - Close[Length])/Close[Length]*100;
	PlotNumeric("ROC",ROCValue);
	PlotNumeric("超买",6.5);
	PlotNumeric("超卖",-6.5);
	PlotNumeric("零线",0);
End


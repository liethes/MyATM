//------------------------------------------------------------------------
// 简称: VOLMA
// 名称: 成交量移动平均
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

Params
	Numeric Length1(5);
	Numeric Length2(22);
	Numeric Length3(60);
	Numeric Length4(250);
Begin 
	PlotNumeric("Vol",Vol);
	PlotNumeric("VolMA1", AverageFC(Vol, Length1));
	PlotNumeric("VolMA2", AverageFC(Vol, Length2));
	PlotNumeric("VolMA3", AverageFC(Vol, Length3));
	PlotNumeric("VolMA4", AverageFC(Vol, Length4));
End


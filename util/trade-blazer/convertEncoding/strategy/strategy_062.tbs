//------------------------------------------------------------------------
// 简称: CMF
// 名称: 佳庆资金流量
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

Params
	Numeric Length(21);
Vars	
	NumericSeries TmpValue;
	Numeric SumVol;
	Numeric CMFValue;
Begin
	If(High > Low)
		TmpValue = ((Close - Low) - (High - Close))/(High - Low)*Vol;
	SumVol = SummationFC(Vol, Length);
	If(SumVol <> 0)	
	CMFValue = SummationFC(TmpValue, Length) /SumVol;
	PlotNumeric("CMF", CMFValue);
	PlotNumeric("Ref",0);
End


//------------------------------------------------------------------------
// 简称: VROC
// 名称: 成交量变动率
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

Params
	Numeric Length(10);
Vars
	Numeric VolTmp;
	Numeric VROCValue(0);
Begin
	VolTmp = Vol[Length];
	If(VolTmp <> 0)
		VROCValue = (Vol - VolTmp) / VolTmp * 100;	
	PlotNumeric("VROC", VROCValue);
	PlotNumeric("零线",0);
End


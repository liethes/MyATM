Params
	Numeric Length1(5);
	Numeric Length2(10);
	Numeric Length3(20); 
	Numeric Length4(30);
Begin
	PlotNumeric("MA1",AverageFC(Close,Length1));
	PlotNumeric("MA2",AverageFC(Close,Length2));
	PlotNumeric("MA3",AverageFC(Close,Length3));
	PlotNumeric("MA4",AverageFC(Close,Length4));
End


//------------------------------------------------------------------------
// 简称: nMins
// 名称: N分钟跨周期引用
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

PARAMS
	Numeric N(3); // N 分钟
BEGIN
	PlotNumeric("Open", DataConvert(Open, "min", N, "Open"));
	PlotNumeric("High", DataConvert(High, "min", N, "High"));
	PlotNumeric("Low", DataConvert(Low, "min", N, "Low"));
	PlotNumeric("Close", DataConvert(Close, "min", N, "Close"));
END


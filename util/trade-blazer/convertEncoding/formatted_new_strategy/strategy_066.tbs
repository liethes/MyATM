PARAMS
	Numeric Length(10);
VARS
	Numeric CORValue(0);
BEGIN
	CORValue = Covar(Close, Data1.Close, Length);
	PlotNumeric("CORA", CORValue);
	PlotNumeric("零线", 0);
END


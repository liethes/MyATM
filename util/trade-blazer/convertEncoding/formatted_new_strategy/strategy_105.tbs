//------------------------------------------------------------------------
// 简称: SAR
// 名称: 抛物线转向
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

PARAMS
	Numeric AfStep(0.02);
	Numeric AfLimit(0.2);
VARS
	Numeric oParCl(0);
	Numeric oParOp(0);
	Numeric oPosition(0);
	Numeric oTransition(0);
BEGIN
	ParabolicSAR(AfStep, AfLimit, oParCl, oParOp, oPosition, oTransition);
	PlotNumeric("ParCl", oParCl);
END


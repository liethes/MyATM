//------------------------------------------------------------------------
// 简称: nDays
// 名称: N天跨周期引用
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

Params
	Numeric N(3);	// N 天
Begin
	PlotNumeric("Open",DataConvert(Open,"day",N,"Open"));
	PlotNumeric("High",DataConvert(High,"day",N,"High"));
	PlotNumeric("Low",DataConvert(Low,"day",N,"Low"));
	PlotNumeric("Close",DataConvert(Close,"day",N,"Close"));
End


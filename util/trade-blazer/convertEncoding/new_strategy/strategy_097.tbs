Params
	Numeric Length(5);
Vars
	NumericSeries HighestValue;
	NumericSeries LowestValue;
Begin
	HighestValue = HighestFC(High,Length);
	LowestValue = LowestFC(Low,Length);
	PlotNumeric("UpChannel",HighestValue[1]);
	PlotNumeric("DownChannel",LowestValue[1]);
End


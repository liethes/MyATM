//------------------------------------------------------------------------
// 简称: CV
// 名称: 佳庆变异率
// 类别: 公式应用
// 类型: 内建应用
//------------------------------------------------------------------------

Params
	Numeric Length(10);
Vars
	NumericSeries HiLoAverage;
	Numeric TmpValue;
	Numeric CVValue;	
Begin
	HiLoAverage = XAverage(High - Low,Length);
	TmpValue = HiLoAverage[Length];
	If(TmpValue <> 0)
	CVValue=(HiLoAverage - TmpValue)/TmpValue*100;
	PlotNumeric("CV",CVValue);
	PlotNumeric("Ref",0);	
End


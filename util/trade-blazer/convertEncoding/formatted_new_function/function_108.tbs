//------------------------------------------------------------------------
// 简称: SwingHigh
// 名称: 求波峰点
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

PARAMS
	Numeric Instance(1);
	NumericSeries Price(1);
	Numeric Strength(1);
	Numeric Length(10);
VARS
	Numeric PivotPrice;
	Numeric PivotBar;
BEGIN
	Pivot(Price, Length, Strength, Strength, Instance, 1, PivotPrice, PivotBar);
	RETURN PivotPrice;
END


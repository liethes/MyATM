//------------------------------------------------------------------------
// 简称: LinearRegSlope
// 名称: 求线性回归斜率
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

PARAMS
	NumericSeries Price(1);
	Numeric Length(10);
VARS
	Numeric LRSlope;
	Numeric LRAngle;
	Numeric LRIntercept;
	Numeric LRValue;
BEGIN
	LinearReg(Price, Length, 0, LRSlope, LRAngle, LRIntercept, LRValue);
	RETURN LRSlope;
END


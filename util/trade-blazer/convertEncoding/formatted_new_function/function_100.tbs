//------------------------------------------------------------------------
// 简称: Skewness
// 名称: 求偏度系数
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

PARAMS
	NumericSeries Price(1);
	Numeric Length(10);
VARS
	Numeric SkewValue(0);
	Numeric Sum(0);
	Numeric Y;
	Numeric Mean;
	Numeric SDev;
	Numeric i;
BEGIN
	if (Length > 2) {
		Mean = Average(Price, Length);
		SDev = StandardDev(Price, Length, 2);
		if (SDev > 0) {
			for i = 0 to Length - 1 {
				Sum = Sum + Power((Price[i] - Mean) / SDev, 3);
			}
			Y = Length / ((Length - 1) * (Length - 2));
			SkewValue = Y * Sum;
		}
	}
	RETURN SkewValue;
END


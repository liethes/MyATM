//------------------------------------------------------------------------
// 简称: LowestBarFC
// 名称: 求最低值出现的Bar(快速计算版本)
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

PARAMS
	NumericSeries Price(1);
	Numeric Length(10);
VARS
	Numeric LowestValue;
	Numeric ExtremesBar;
BEGIN
	LowestValue = Extremes(Price, Length, false, ExtremesBar);
	RETURN ExtremesBar;
END


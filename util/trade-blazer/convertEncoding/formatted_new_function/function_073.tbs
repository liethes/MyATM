//------------------------------------------------------------------------
// 简称: NormalCumDensity
// 名称: 求指定数据的正态分布累计概率密度
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

PARAMS
	NumericSeries Price(1);
	Numeric Length(10);
VARS
	Numeric Mean;
	Numeric StdDev;
BEGIN
	Mean = Average(Price, Length);
	StdDev = StandardDev(Price, Length, 2);
	if (StdDev > 0) {
		RETURN NormalSCDensity((Price - Mean) / StdDev);
	}
	RETURN - 1;
END


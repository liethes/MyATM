//------------------------------------------------------------------------
// 简称: LinearRegValue
// 名称: 求线性回归值
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

Params
	NumericSeries Price(1);
	Numeric Length(10);
	Numeric TgtBar(0);
Vars
	Numeric LRSlope; 
	Numeric LRAngle;
	Numeric LRIntercept;
	Numeric LRValue;
Begin
	LinearReg(Price, Length, TgtBar, LRSlope, LRAngle, LRIntercept, LRValue);
	Return LRValue;
End


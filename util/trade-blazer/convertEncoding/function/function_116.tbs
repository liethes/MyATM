//------------------------------------------------------------------------
// 简称: TrueRange
// 名称: 求真实范围
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

Begin
	If(CurrentBar == 0)
		Return High - Low;
	Else
		Return TrueHigh - TrueLow;
End


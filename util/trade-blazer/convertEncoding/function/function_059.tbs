//------------------------------------------------------------------------
// 简称: LowD
// 名称: 求N天前的最低价
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

Params
	Numeric daysAgo(2);
Vars
	NumericSeries barCnt;
	NumericSeries dayLow;
	Numeric i;
	Numeric j;
	Numeric nIndex(0);
	Numeric CBIndex;
Begin
	CBIndex = CurrentBar;
	If(CBIndex == 0 || TrueDate(0)!=TrueDate(1))
	{
		barCnt = 1;
		dayLow = Low;
	}Else
	{
		barCnt = barCnt + 1;
		If(Low < dayLow)
			dayLow = Low;
	}
	
	If(daysAgo == 0)
	{
		return dayLow;
	}Else
	{
		For i = 1 To daysAgo
		{
			If( i == 1)
			{	
				j = 0;
			}Else
			{
				j = j + BarCnt[j];				
			}
			If (j > CBIndex ) 
				Return InvalidNumeric;
			nIndex = nIndex + BarCnt[j];
		}
		Return dayLow[nIndex];
	}
End


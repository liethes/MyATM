//------------------------------------------------------------------------
// 简称: Permutation
// 名称: 求排列
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

Params
	Numeric Num(5);
	Numeric NumChosen(3);
Vars
	Numeric PermValue;
Begin
	If (NumChosen >= 1 And Num >= NumChosen)
	{
		PermValue = Fact(Num)/Fact(Num - IntPart(NumChosen));
	}Else
	{
		PermValue = -1;
	}
	Return PermValue;
End


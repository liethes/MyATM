//------------------------------------------------------------------------
// 简称: PercentChange
// 名称: 求涨跌幅
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

Params
	NumericSeries Price(1);
	Numeric Length(10);
Vars
	Numeric PCValue(0);
Begin
	If (Price[Length] != 0 )
	{
		PCValue = ( Price - Price[Length] ) / Price[Length];
	}Else
	{
		PCValue = 0;
	}
	Return PCValue;
End


//------------------------------------------------------------------------
// 简称: Fisher
// 名称: 求Fisher变换
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

Params
	NumericSeries Price(1);
Vars
	Numeric FisherValue;
Begin
	if ((Price > -1) And (Price < 1))
	{
		FisherValue = Ln(( 1 + Price ) / ( 1 - Price )) * 0.5;
	}Else
	{
		FisherValue =  -999 ;	
	}	
	Return FisherValue;
End


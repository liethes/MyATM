//------------------------------------------------------------------------
// 简称: Intrinsic
// 名称: 获取期权的内在价值
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

Params
	Numeric AssetPr(0);			// 期权标的的价格
	Numeric PutCall(0);			// OptionType 0 - Put, 1 - Call
	Numeric StrikePr(0);		// 期权的执行价
Begin
	If (PutCall == Enum_CallOption)
	{
		Return Max(AssetPr - StrikePr, 0);
	}Else
	{
		Return Max(StrikePr - AssetPr, 0);
	}
End


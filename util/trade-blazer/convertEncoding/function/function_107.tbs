//------------------------------------------------------------------------
// 简称: SummationFC
// 名称: 快速求和
// 类别: 用户函数
// 类型: 内建函数
// 输出: 数值型
//------------------------------------------------------------------------

Params
	NumericSeries Price(1);
	Numeric Length(10);
Vars
	NumericSeries SumValue(0);
	Numeric i;
Begin
	If(CurrentBar < Length)
	{
		SumValue = 0;
		for i = 0 to Length - 1
		{
			SumValue = SumValue + Price[i];
		}
	}Else
	{
		SumValue = SumValue[1] + Price - Price[Length];
	}
	Return SumValue;
End

